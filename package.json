{
  "name": "typeioc",
  "version": "3.0.0",
  "scripts": {
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc -b src test",
    "lint:src": "tslint -c tslint.json -p src/tsconfig.json",
    "lint:test": "tslint -c tslint.json -p test/tsconfig.json",
    "lint": "npm run lint:src && npm run lint:test",
    "clean": "rimraf ./lib",
    "test": "cross-env TS_NODE_FILES=true TS_NODE_PROJECT=\"./test/tsconfig.json\" tap --node-arg=\"-r\" --node-arg=\"tsconfig-paths/register\" test/tests/**",
    "test:watch": "npm run test -- --watch --changed",
    "test:report": "npm run test -- --coverage-report=\"html\"",
    "test:100": "npm run test -- --100",
    "extract-api": "api-extractor run --local --verbose",
    "docs:api:update": "npm run build && npm run extract-api && npm run docs:api && git checkout ./api-metadata/docs/index.md",
    "docs:api": "api-documenter markdown -i ./api-metadata/temp -o ./api-metadata/docs",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:build": "gitbook build",
    "docs:publish": "rm _book -rf && npm run docs:build && cd _book && git init && git config user.email \"german.maxim@gmail.com\" && git config user.name \"Max Gherman\" && git remote add origin https://github.com/maxgherman/TypeIOC-docs.git && git add . && git commit -am 'Update docs' && git push origin master --force"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "directories": {
    "lib": "./lib"
  },
  "dependencies": {
    "hashes": "0.1.4",
    "reflect-metadata": "0.1.13",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "7.3.2",
    "@microsoft/api-extractor": "7.2.0",
    "@types/node": "12.0.8",
    "@types/sinon": "7.0.12",
    "cross-env": "5.2.0",
    "gitbook-plugin-anchorjs": "2.0.1",
    "gitbook-plugin-github": "2.0.0",
    "gitbook-plugin-prism": "2.4.0",
    "gitbook-plugin-sectionx": "3.1.0",
    "rimraf": "2.6.3",
    "sinon": "7.3.2",
    "tap": "14.2.2",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.17.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "3.5.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "description": "Dependency injection container for node typescript / javascript",
  "homepage": "https://github.com/typeioc/TypeIOC",
  "keywords": [
    "typescript",
    "dependency",
    "injection",
    "ioc",
    "container",
    "inversion",
    "control"
  ],
  "author": {
    "name": "Max Gherman",
    "email": "german.maxim@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typeioc/TypeIOC.git"
  },
  "bugs": {
    "email": "german.maxim@gmail.com",
    "url": "https://github.com/typeioc/TypeIOC/issues"
  },
  "license": "MIT",
  "readmeFilename": "README.md"
}
