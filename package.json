{
  "name": "typeioc",
  "version": "2.1.4",
  "description": "Dependency injection container for node typescript / javascript",
  "homepage": "https://github.com/maxgherman/TypeIOC",
  "main": "./index",
  "directories": {
    "lib": "./lib"
  },
  "dependencies": {
    "hashes": "0.1.4",
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "@types/node": "9.6.7",
    "coveralls": "3.0.0",
    "rimraf": "2.6.2",
    "gitbook-cli": "2.3.2",
    "gitbook-plugin-anchorjs": "1.1.1",
    "gitbook-plugin-github": "2.0.0",
    "gitbook-plugin-prism": "2.3.0",
    "gitbook-plugin-sectionx": "3.1.0",
    "istanbul": "0.4.5",
    "nodeunit": "0.11.2",
    "sinon": "4.5.0",
    "typescript": "2.8.3"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "prebuild": "rimraf lib/*",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prebuild:test": "rimraf test/js/*.{js,map} test/js/{data,ts}/*",
    "build:test": "tsc -p tsconfig.tests.json",
    "test": "node ./node_modules/nodeunit/bin/nodeunit test/js/internal/ test/js/api/ test/js/ts/",
    "coverage": "NODE_ENV=coverage node node_modules/.bin/istanbul cover node_modules/.bin/nodeunit test/js/internal/ test/js/api/ test/ts/",
    "coveralls": "cat test/coverage/lcov.info | coveralls",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:build": "gitbook build",
    "docs:publish": "rm _book -rf && npm run docs:build && cd _book && git init && git remote add origin https://github.com/maxgherman/TypeIOC-docs.git && git add . && git commit -am 'Update docs' && git push origin master --force"
  },
  "keywords": [
    "typescript",
    "dependency",
    "injection",
    "ioc",
    "container",
    "inversion",
    "control"
  ],
  "author": {
    "name": "Max Gherman",
    "email": "german.maxim@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/maxgherman/TypeIOC.git"
  },
  "bugs": {
    "email": "german.maxim@gmail.com",
    "url": "https://github.com/maxgherman/TypeIOC/issues"
  },
  "license": "MIT",
  "readmeFilename": "README.md"
}
